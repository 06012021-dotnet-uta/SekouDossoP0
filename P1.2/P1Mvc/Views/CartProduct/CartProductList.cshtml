@model IEnumerable<ModelsLayer.CartProduct>

@{
    ViewData["Title"] = "CartProduct";
}

<h1>CartProduct</h1>

<p>
    <a class="navbar-brand" asp-area="" asp-controller="Main" asp-action="Index">Home</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CartId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProductId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CartId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProductId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CartproductId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CartproductId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CartproductId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
